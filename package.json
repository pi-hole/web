    "nprogress": "0.2.0",
    "qrious": "4.0.2",
    "select2": "4.0.13"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-compat": "^6.0.2",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "xo": "^1.1.1"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "not op_mini all",
    "Firefox ESR"
